{
  "name": "blue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "nodemon": "cross-env dev=true nodemon server/www/index.ts",
    "node": "cross-env nodemon server/www/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm",
    "entities": "npm run typeorm -- entity:create --config server/config/typeorm.ts --connection -d server/typeorm/entity --name",
    "create": "npm run typeorm -- migration:create --config server/config/typeorm.ts --connection -d server/typeorm/migrations --name 'migrations'",
    "generate": "npm run typeorm -- migration:generate --config server/config/typeorm.ts --connection -d server/typeorm/migrations --name 'migrations'",
    "jest": "cross-env dev=true test=true jest server/__test__/*.test.ts && cat test.log",
    "reset": "ts-node reset.ts"
  },
  "author": "nocturnal (Kenedi Novriansyah)",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cowsay": "^1.4.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "faker": "^5.5.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.32",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.1",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
